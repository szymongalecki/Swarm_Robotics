<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>// Place global declarations here.
</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<parameter>bool turned_random, bool turned_180, bool connected, int x, int y, int x_dir, int y_dir</parameter>
		<declaration>// Place local declarations here.
</declaration>
		<location id="id0" x="-544" y="-93">
			<name x="-554" y="-127">initial</name>
			<committed/>
		</location>
		<location id="id1" x="-340" y="-93">
			<name x="-350" y="-127">turn_random</name>
			<committed/>
		</location>
		<location id="id2" x="-25" y="76">
			<name x="-34" y="42">turn_180</name>
			<committed/>
		</location>
		<location id="id3" x="-34" y="-93">
			<name x="-59" y="-136">forward</name>
			<committed/>
		</location>
		<location id="id4" x="459" y="-93">
			<name x="449" y="-127">final</name>
			<committed/>
		</location>
		<init ref="id0"/>
		<transition id="id5">
			<source ref="id3"/>
			<target ref="id3"/>
			<label kind="guard" x="-212" y="-297">(turned_random == true &amp;&amp; connected == true) 
|| (turned_180 == true &amp;&amp; connected == false)</label>
			<label kind="assignment" x="-76" y="-229">x += x_dir,
y += y_dir</label>
			<nail x="-255" y="-246"/>
			<nail x="204" y="-246"/>
		</transition>
		<transition id="id6">
			<source ref="id3"/>
			<target ref="id4"/>
			<label kind="guard" x="59" y="-76">(turned_random &amp;&amp; turned_180 &amp;&amp; connected ) 
== true</label>
		</transition>
		<transition id="id7">
			<source ref="id2"/>
			<target ref="id3"/>
			<nail x="-246" y="76"/>
		</transition>
		<transition id="id8">
			<source ref="id3"/>
			<target ref="id2"/>
			<nail x="221" y="76"/>
		</transition>
		<transition id="id9">
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="assignment" x="-323" y="-76">turned_random = true</label>
			<nail x="-85" y="-93"/>
		</transition>
		<transition id="id10">
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="-535" y="-76">turned_random = false,
turned_180 = false,
connected = true,
x = 0,
y = 0,
x_dir = 0,
y_dir = 0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
</system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
